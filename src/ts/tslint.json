{
    "extends": "tslint:recommended",
    "rules": {
        "arrow-parens": false,
        "curly": false,
        "interface-name": [
            false
        ],
        "no-console": [
            false
        ],
        "quotemark": [
            "single"
        ],
        "trailing-comma": [
            false
        ],
        "align": [
            "arguments",
            "statements"
        ]
    }
}